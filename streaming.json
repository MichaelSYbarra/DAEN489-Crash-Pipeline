{
  "mode": "streaming",
  "source": "crash",
  "join_key": "crash_record_id",
  "primary": {
    "id": "85ca-t3if",
    "alias": "crashes",
    "where_by": {"since_days": 7},
    "order": "crash_date, crash_record_id",
    "page_size": 2000,
    "select": "crash_record_id,crash_date,crash_type,injuries_total,trafficway_type,weather_condition,lighting_condition,lane_cnt,roadway_surface_cond,road_defect,beat_of_occurrence"
  },
  "enrich": [
    {
      "id": "68nd-jvt3",
      "alias": "vehicles",
      "select": "crash_record_id,unit_no,make,model,vehicle_year,maneuver,vehicle_defect,vehicle_use"
    },
    {
      "id": "u6pd-qa9d",
      "alias": "people",
      "select": "crash_record_id,person_id,person_type,age,sex,injury_classification,safety_equipment,airbag_deployed"
    }
  ],
  "batching": {
    "id_batch_size": 50,
    "max_workers": { "vehicles": 4, "people": 4 }
  },
  "storage": {
    "bucket": "raw-data",
    "prefix": "crash",
    "compress": true
  }
}